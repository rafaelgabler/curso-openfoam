FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField

{
    topWall
    {
        type            nutkWallFunction;
        value           uniform 0;
    }

    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    lowerWall
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    Obstacle
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    ObstacleLeft
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    ObstacleRight
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
}
